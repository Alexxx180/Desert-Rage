        <Image x:Name="ChestImg4" Grid.ColumnSpan="1" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Panel.ZIndex="3" Source="/Resources/Images/Locations/Total/Chests/ChestClosed(ver1).png" IsEnabled="False" Visibility="Hidden" Grid.Row="8" Grid.Column="35" Stretch="Fill"/>
        <Image x:Name="SecretChestImg1" Grid.ColumnSpan="1" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Panel.ZIndex="3" Source="/Resources/Images/Locations/Total/Chests/ChestClosed(ver2).png" IsEnabled="False" Visibility="Hidden" Grid.Row="27" Grid.Column="28" Stretch="Fill"/>
        <Image x:Name="SecretChestImg2" Grid.ColumnSpan="1" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Panel.ZIndex="3" Source="/Resources/Images/Locations/Total/Chests/ChestClosed(ver2).png" IsEnabled="False" Visibility="Hidden" Grid.Row="5" Grid.Column="56" Stretch="Fill"/>
