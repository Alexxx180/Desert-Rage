        <MediaElement x:Name="Sound2" Volume="{Binding ElementName=NoiseLoud, Path=Value}" LoadedBehavior="Manual" HorizontalAlignment="Left" Height="25" Margin="0,0,0,0" VerticalAlignment="Center" Width="25" Source="/Resources/OST/Noises/Punch.mp3" IsEnabled="False" Visibility="Hidden" MediaEnded="Sounds_Ended" />
        <MediaElement x:Name="Sound3" Volume="{Binding ElementName=SoundsLoud, Path=Value}" HorizontalAlignment="Left" Margin="0,0,0,0" LoadedBehavior="Manual" UnloadedBehavior="Manual" VerticalAlignment="Top" Grid.Column="3" Source="/Resources/OST/Sounds/ChestOpened.mp3" MediaEnded="Sounds_Ended" />

        <!--Авторизация-->
        <Grid x:Name="MainMenu" Grid.ColumnSpan="60" Grid.RowSpan="36" ZIndex="4" Visibility="Visible">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0.24*"></ColumnDefinition>
                <ColumnDefinition Width="0.06*"></ColumnDefinition>
                <ColumnDefinition Width="0.4*"></ColumnDefinition>
                <ColumnDefinition Width="0.3*"></ColumnDefinition>
            </Grid.ColumnDefinitions>

            <Grid.RowDefinitions>
                <RowDefinition Height="0.03*"></RowDefinition>
                <RowDefinition Height="0.17*"></RowDefinition>
                <RowDefinition Height="0.205*"></RowDefinition>
                <RowDefinition Height="0.07*"></RowDefinition>
                <RowDefinition Height="0.125*"></RowDefinition>
                <RowDefinition Height="0.025*"></RowDefinition>
                <RowDefinition Height="0.125*"></RowDefinition>
                <RowDefinition Height="0.05*"></RowDefinition>
            </Grid.RowDefinitions>
            <Image x:Name="MenuBackground" HorizontalAlignment="Left" VerticalAlignment="Top" Stretch="Fill" Source="{Binding ElementName=Img1, Path=Source}" Grid.Column="0" Visibility="Visible" Grid.RowSpan="8" Grid.ColumnSpan="4"/>

            <Grid Grid.RowSpan="3">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="0.15*"></ColumnDefinition>
                    <ColumnDefinition Width="0.7*"></ColumnDefinition>
                    <ColumnDefinition Width="0.15*"></ColumnDefinition>
                </Grid.ColumnDefinitions>

                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <Viewbox Grid.Row="0" Grid.Column="1" Panel.ZIndex="11">
                    <Button x:Name="CurrentPlayer" Content="PLAYER" HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Center" FontFamily="Bernard MT Condensed" FontSize="36" Panel.ZIndex="5" Grid.Column="1" Visibility="Visible" Foreground="White" Click="ListPlayers" Background="{x:Null}"  BorderBrush="#FF611717" Padding="5,0,5,0" BorderThickness="2" />
                </Viewbox>

                <Viewbox Grid.Row="1" Grid.Column="1" Panel.ZIndex="11">
                    <Button x:Name="Player1" Content="CHECK0000000001" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Center" FontFamily="Bernard MT Condensed" FontSize="36" Panel.ZIndex="5" Grid.Column="1" Visibility="Hidden" Foreground="White" Grid.Row="1" IsEnabled="False" Click="PlayersSelecting" Background="{x:Null}"  BorderBrush="#FF611717" Padding="5,0,5,0" />
                </Viewbox>

                <Viewbox Grid.Row="2" Grid.Column="1" Panel.ZIndex="11">
                    <Button x:Name="Player2" Content="CHECK0000000002" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Center" FontFamily="Bernard MT Condensed" FontSize="36" Panel.ZIndex="5" Grid.Column="1" Visibility="Hidden" Foreground="White" Grid.Row="2" IsEnabled="False" Click="PlayersSelecting" Background="{x:Null}"  BorderBrush="#FF611717" Padding="5,0,5,0" />
                </Viewbox>

                <Viewbox Grid.Row="3" Grid.Column="1" Panel.ZIndex="11">
                    <Button x:Name="Player3" Content="CHECK0000000003" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Center" FontFamily="Bernard MT Condensed" FontSize="36" Panel.ZIndex="5" Grid.Column="1" Visibility="Hidden" Foreground="White" Grid.Row="3" IsEnabled="False" Click="PlayersSelecting" Background="{x:Null}"  BorderBrush="#FF611717" Padding="5,0,5,0" />
                </Viewbox>

                <Viewbox Grid.Row="4" Grid.Column="1" Panel.ZIndex="11">
                    <Button x:Name="Player4" Content="CHECK0000000004" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Center" FontFamily="Bernard MT Condensed" FontSize="36" Panel.ZIndex="5" Grid.Column="1" Visibility="Hidden" Foreground="White" Grid.Row="4" IsEnabled="False" Click="PlayersSelecting" Background="{x:Null}"  BorderBrush="#FF611717" Padding="5,0,5,0" />
                </Viewbox>

                <Viewbox Grid.Row="5" Grid.Column="1" Panel.ZIndex="11">
                    <Button x:Name="Player5" Content="CHECK0000000005" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Center" FontFamily="Bernard MT Condensed" FontSize="36" Panel.ZIndex="5" Grid.Column="1" Visibility="Hidden" Foreground="White" Grid.Row="5" IsEnabled="False" Click="PlayersSelecting" Background="{x:Null}"  BorderBrush="#FF611717" Padding="5,0,5,0" />
                </Viewbox>

                <Viewbox Grid.Row="6" Grid.Column="1" Panel.ZIndex="11">
                    <Button x:Name="Player6" Content="CHECK0000000006" HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Center" FontFamily="Bernard MT Condensed" FontSize="36" Panel.ZIndex="5" Grid.Column="1" Visibility="Hidden" Foreground="White" Grid.Row="6" IsEnabled="False" Click="PlayersSelecting" Background="{x:Null}"  BorderBrush="#FF611717" Padding="5,0,5,0" />
                </Viewbox>

                <Image x:Name="AutorizeImg" HorizontalAlignment="Left" VerticalAlignment="Top" Stretch="Uniform" Source="/Resources/Images/Autorize/Wanted.png" Grid.Column="0" Grid.Row="0" Visibility="Visible" Grid.ColumnSpan="15" Grid.RowSpan="18" Panel.ZIndex="4"/>

                <Grid Grid.Column="1" Grid.Row="7" Grid.ColumnSpan="2" Panel.ZIndex="5" Visibility="Hidden" IsEnabled="False">
                    <TextBox x:Name="AddPlayer" Text="" HorizontalAlignment="Stretch" MaxLength="15" Margin="0,0,0,0" TextWrapping="Wrap" VerticalAlignment="Stretch" FontFamily="Bernard MT Condensed" PreviewTextInput="AddPlayer_PreviewTextInput" Visibility="Visible" Foreground="#FFE29C20" SelectionBrush="#FFE29C20" BorderBrush="{x:Null}" IsEnabled="True" />
                    <Viewbox HorizontalAlignment="Left" IsHitTestVisible="False">
                        <Label Foreground="Black" HorizontalAlignment="Stretch" Content="{Binding ElementName=AddPlayer, Path=Text}" FontFamily="Bernard MT Condensed" Padding="0"></Label>
                    </Viewbox>
                </Grid>

                <Viewbox x:Name="NobodyNotAllowed" Grid.Row="7" Grid.Column="1" Grid.RowSpan="1" Grid.ColumnSpan="2" Panel.ZIndex="11" IsHitTestVisible="False" HorizontalAlignment="Stretch" Visibility="Collapsed">
                    <Label Grid.Column="1" HorizontalAlignment="Stretch" Margin="0,0,0,0" Grid.Row="7" VerticalAlignment="Stretch" Panel.ZIndex="5" FontSize="36" FontFamily="Franklin gothic demi" PreviewTextInput="AddPlayer_PreviewTextInput" Visibility="Visible" IsEnabled="False" Grid.ColumnSpan="2" Content="Никем быть нельзя!" IsHitTestVisible="False" Background="#B2000000" Foreground="White" />
                </Viewbox>
                <Viewbox x:Name="AlreadyHaveOne"  Grid.Row="7" Grid.Column="1" Grid.RowSpan="1" Grid.ColumnSpan="2" Panel.ZIndex="11" IsHitTestVisible="False" HorizontalAlignment="Stretch" Visibility="Hidden">
                    <Label Grid.Column="1" HorizontalAlignment="Stretch" Margin="0,0,0,0" Grid.Row="7" VerticalAlignment="Stretch" Panel.ZIndex="5" FontSize="36" FontFamily="Franklin gothic demi" Background="#B2000000" PreviewTextInput="AddPlayer_PreviewTextInput" Visibility="Visible" IsEnabled="False" Grid.ColumnSpan="2" Content="Один уже есть!"  Foreground="White" />
                </Viewbox>


                <Button x:Name="AddProfile" HorizontalAlignment="Stretch" Margin="0,0,0,0" VerticalAlignment="Stretch" FontSize="48" Click="AddProfile_Click" Panel.ZIndex="5" Grid.Row="7" Grid.Column="0" Visibility="Hidden" Background="#FF77FFA3" BorderBrush="White" IsEnabled="False">
                    <Button.Content>
                        <Viewbox>
                            <Label  Foreground="White" FontFamily="Arial Black">+</Label>
                        </Viewbox>
                    </Button.Content>
                </Button>

                <Button x:Name="DeleteProfile" HorizontalAlignment="Stretch" Margin="0,0,0,0" VerticalAlignment="Stretch" FontSize="48" Click="DeleteProfile_Click" Panel.ZIndex="5" Visibility="Hidden" Background="#FFFF1818" BorderBrush="White" IsEnabled="False" Grid.Row="6">
                    <Button.Content>
                        <Viewbox>
                            <Label Foreground="White" FontFamily="Arial Black" >X</Label>
                        </Viewbox>
                    </Button.Content>
                </Button>
            </Grid>

            <Viewbox Grid.Row="1" Grid.Column="2" Panel.ZIndex="11">
                <Label x:Name="Lab1" Content="Desert Rage" HorizontalAlignment="Stretch" Margin="0,0,0,0" VerticalAlignment="Stretch" FontFamily="Bauhaus 93" FontSize="132" Panel.ZIndex="4" Grid.Column="2" Grid.Row="1" Visibility="Visible" BorderBrush="#FF611717" BorderThickness="8,8,8,8">
                    <Label.Foreground>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFFB9809" Offset="0"/>
                            <GradientStop Color="Black" Offset="1"/>
                        </LinearGradientBrush>
                    </Label.Foreground>
                    <Label.Background>
                        <RadialGradientBrush GradientOrigin="1,1" Center="0.75,1.5" RadiusX="1" RadiusY="0.75">
                            <GradientStop Color="#D8000000" Offset="1"/>
                            <GradientStop Color="#FFD47C11" Offset="0.6"/>
                        </RadialGradientBrush>
                    </Label.Background>
                </Label>
            </Viewbox>

            <Button x:Name="NewAdventure" HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Center" FontSize="72" Click="NewAdventure_Click" Panel.ZIndex="5" Visibility="Visible" FontStretch="Normal" MouseEnter="GameStartBtns_MouseEnter" MouseLeave="GameStartBtns_MouseLeave" BorderBrush="#FF612817" Grid.Row="6" Grid.Column="2">
                <Image x:Name="NewAdv" Source="/Resources/Images/Autorize/Adventures3.png" Stretch="Fill" />
            </Button>
            <Button x:Name="Continue" HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Center" FontSize="72" Click="Continue_Click" Panel.ZIndex="5" Visibility="Visible" IsEnabled="False" FontStretch="Normal" MouseLeave="GameStartBtns_MouseLeave" MouseEnter="GameStartBtns_MouseEnter" BorderBrush="#FF612817" Grid.Row="4" Grid.Column="2">
                <Image x:Name="ConAdv" Source="/Resources/Images/Autorize/Adventures4.png" Stretch="Fill" />
            </Button>

        </Grid>

        <!--Сцены-->
        <Grid Grid.ColumnSpan="60" Grid.RowSpan="36" Panel.ZIndex="16">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0.82*"></ColumnDefinition>
                <ColumnDefinition Width="0.15*"></ColumnDefinition>
                <ColumnDefinition Width="0.03*"></ColumnDefinition>
            </Grid.ColumnDefinitions>

            <Grid.RowDefinitions>
                <RowDefinition Height="0.61*"></RowDefinition>
                <RowDefinition Height="0.09*"></RowDefinition>
                <RowDefinition Height="0.22*"></RowDefinition>
                <RowDefinition Height="0.08*"></RowDefinition>
            </Grid.RowDefinitions>

            <MediaElement x:Name="Med2" LoadedBehavior="Manual" UnloadedBehavior="Manual" Grid.ColumnSpan="4" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" IsEnabled="False" Visibility="Hidden" Source="Resources/CutScenes/BattleStarts/Battlestations1.mp4" MediaEnded="Med2_MediaEnded" SpeedRatio="1" Stretch="Fill" Grid.RowSpan="4" MediaFailed="MediaErrorEncountered" IsMuted="True" Volume="0"/>
            <MediaElement x:Name="Win" Grid.ColumnSpan="4" HorizontalAlignment="Left" LoadedBehavior="Manual" VerticalAlignment="Top" Margin="0,0,0,0" Source="Resources/CutScenes/BattleEnds/Win1.mp4" Visibility="Hidden" IsEnabled="False" MediaEnded="Win_MediaEnded" UnloadedBehavior="Manual" MediaFailed="Win_MediaFailed" Stretch="Fill" MediaOpened="Win_MediaOpened" Grid.RowSpan="4"/>
            <MediaElement x:Name="Med1" LoadedBehavior="Manual" Stretch="Fill"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Visibility="Hidden" Source="Resources\CutScenes\ChaptersIntroduction\Prologue.mp4" MediaFailed="MediaErrorEncountered" MediaEnded="Med1_MediaEnded" Grid.Column="0" Grid.Row="0" Margin="0,0,0,0" MediaOpened="Med1_MediaOpened" Grid.ColumnSpan="4" Grid.RowSpan="4" IsEnabled="False" />
            <MediaElement x:Name="TheEnd" Grid.ColumnSpan="4" HorizontalAlignment="Left" VerticalAlignment="Top" Visibility="Hidden" LoadedBehavior="Manual" UnloadedBehavior="Manual" IsEnabled="False" MediaEnded="TheEnd_MediaEnded" Source="Resources\CutScenes\BattleEnds\Win1.mp4" Stretch="Fill" SpeedRatio="2" Grid.RowSpan="4" MediaFailed="MediaErrorEncountered"/>
            <MediaElement x:Name="GameOver" Grid.ColumnSpan="4" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" LoadedBehavior="Manual" Visibility="Hidden" IsEnabled="False" Source="Resources\CutScenes\BattleEnds\GameOver.mp4" MediaEnded="GameOver_MediaEnded" Grid.RowSpan="4"/>
            <MediaElement x:Name="ChapterIntroduction" LoadedBehavior="Manual" Stretch="Fill"  HorizontalAlignment="Left" VerticalAlignment="Top" Visibility="Hidden" Source="Resources/CutScenes/ChaptersIntroduction/Chapter1.mp4" MediaEnded="ChapterIntroduction_MediaEnded" Grid.Column="0" Grid.Row="0" Margin="0,0,0,0" Grid.ColumnSpan="4" Grid.RowSpan="4" IsEnabled="False" MediaFailed="MediaErrorEncountered" />
            <Button x:Name="Skip1" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" FontFamily="Franklin Gothic Demi Cond" FontSize="48" FontStyle="Italic" Click="Skip1_Click" Visibility="Hidden" IsEnabled="False" Grid.Column="1" Grid.Row="1" MouseEnter="Skip1_MouseEnter" MouseLeave="Skip1_MouseLeave" BorderBrush="#FF612817" Grid.ColumnSpan="1">
                <Image x:Name="SkipImg" Source="/Resources/Images/Autorize/Skips1.png" Stretch="Fill" />
            </Button>
            <Viewbox x:Name="AutoSave" Grid.Row="3" Grid.Column="1" HorizontalAlignment="Right" Grid.ColumnSpan="2" Visibility="Hidden">
                <Label Content="Автосохранение" FontFamily="Franklin Gothic Demi" Foreground="WhiteSmoke" Background="#CC000000" BorderBrush="#FFE46800" BorderThickness="1" />
            </Viewbox>
        </Grid>

        <!--Карта-->
        <Viewbox Grid.Row="32" Grid.Column="53" Grid.RowSpan="4" Grid.ColumnSpan="7" Panel.ZIndex="6" HorizontalAlignment="Right">
            <Label x:Name="TimerFlees" Content="2:30" HorizontalAlignment="Right" Margin="0,0,0,0" VerticalAlignment="Bottom" FontFamily="Bauhaus 93" FontSize="72" Panel.ZIndex="7" Grid.Column="53" Grid.Row="32" Visibility="Hidden" Grid.ColumnSpan="7" Grid.RowSpan="4" BorderBrush="#FF00DCFF" BorderThickness="8,8,8,8" Foreground="White" />
        </Viewbox>

        <!--Модели на карте-->
        <Image x:Name="Img1" HorizontalAlignment="Left" VerticalAlignment="Top" Stretch="Fill" Source="/Resources/Images/Locations/Regular/New_game_show.jpg" Grid.Column="0" Grid.Row="0" Visibility="Hidden" Grid.ColumnSpan="60" Grid.RowSpan="36" Panel.ZIndex="3" IsEnabled="False"/>
        <ProgressBar x:Name="HPhelper" Grid.Row="33" Grid.Column="17" Grid.ColumnSpan="3" Value="{Binding ElementName=HPbar1, Path=Value}" Minimum="0" Maximum="{Binding ElementName=HPbar1, Path=Maximum}" Visibility="Hidden" Panel.ZIndex="5">
            <ProgressBar.Foreground>
                <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                    <GradientStop Color="#FF3AAC28"/>
                    <GradientStop Color="#FF00F3FF" Offset="1"/>
                    <GradientStop Color="#FF0EDEAF" Offset="0.512"/>
                </LinearGradientBrush>
            </ProgressBar.Foreground>
        </ProgressBar>
        <Image x:Name="Img2" Grid.Column="18" Grid.Row="34" Margin="0,0,0,0" VerticalAlignment="Center" Source="/Resources/Images/Locations/Total/Person/Static/Down.png" Panel.ZIndex="5" Visibility="Hidden" IsEnabled="False" Stretch="Fill" HorizontalAlignment="Center"/>
