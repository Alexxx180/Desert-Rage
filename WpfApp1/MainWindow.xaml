        <Image x:Name="Table2" Grid.ColumnSpan="1" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Source="/Resources/Images/Locations/Total/Table.png" Stretch="Fill" IsEnabled="False" Visibility="Hidden" Grid.Row="22" Grid.Column="23" Panel.ZIndex="3"/>
        <Image x:Name="Table3" Grid.ColumnSpan="1" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Source="/Resources/Images/Locations/Total/Table.png" Stretch="Fill" Visibility="Hidden" IsEnabled="False" Grid.Row="22" Grid.Column="2" Panel.ZIndex="3"/>
        <Image x:Name="ChestImg1" Grid.ColumnSpan="1" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Source="/Resources/Images/Locations/Total/Chests/ChestClosed(ver1).png" Panel.ZIndex="3" Visibility="Hidden" IsEnabled="False" Grid.Row="10" Grid.Column="8" Stretch="Fill"/>
