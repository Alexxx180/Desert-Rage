        <Image x:Name="KeyImg2" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Panel.ZIndex="3" Source="/Resources/Images/Locations/Loc1/Models/Key.png" IsEnabled="False" Visibility="Hidden" Stretch="Fill" Grid.Row="21" Grid.Column="11"/>
        <Image x:Name="KeyImg3" Grid.ColumnSpan="1" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Panel.ZIndex="3" Source="/Resources/Images/Locations/Loc1/Models/Key.png" Visibility="Hidden" IsEnabled="False" Stretch="Fill" Grid.Column="42" Grid.Row="33"/>
        <Image x:Name="LockImg1" Grid.ColumnSpan="1" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Source="/Resources/Images/Locations/Loc1/Models/Lock.png" Panel.ZIndex="3" IsEnabled="False" Visibility="Hidden" Stretch="Fill" Grid.Row="18" Grid.Column="4"/>
